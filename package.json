    "debounce": "^1.2.0",
    "gsap": "^3.6.1",
    "jquery": "^3.5.1",
    "jquery-match-height": "^0.7.2",
    "jquery-ui-dist": "^1.12.1",
    "jquery-ui-touch-punch": "^0.2.3",
    "macy": "^2.5.1",
    "masonry-layout": "^4.2.2",
    "swiper": "^7.4.1",
    "yarn": "^1.22.0"
  }
